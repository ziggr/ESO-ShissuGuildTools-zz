			  </Texture>

        <Backdrop name="$(parent)_NoteTitle" inherits="ZO_EditBackdrop">
          <Anchor point="TOPLEFT" relativeTo="$(parent)_Line" offsetX="260" offsetY="10" />
          <Dimensions x="298" y="30" />
			    <Controls>
            <EditBox name="$(parent)Text" inherits="ZO_DefaultEditForBackdrop">
					    <OnEnter>SGT_Notebook_NoteText:TakeFocus()</OnEnter>
				      <OnTab>SGT_Notebook_NoteText:TakeFocus()</OnTab>
				      <OnEscape>self:LoseFocus()</OnEscape>
			      </EditBox>
			    </Controls>
				</Backdrop>

				<Button name="$(parent)_Color1" inherits="ZO_DefaultButton" text="1">
					<Anchor point="TOPLEFT" relativePoint="TOPLEFT" offsetY="40" relativeTo="$(parent)_NoteTitle" />
					<Dimensions x="30" y="30" />
					<MouseButton enabled="true" />
				</Button>

        <Button name="$(parent)_Color2" inherits="ZO_DefaultButton" text="2">
					<Anchor point="TOPLEFT" relativePoint="TOPLEFT" offsetX="40" relativeTo="$(parent)_Color1" />
					<Dimensions x="30" y="30" />
					<MouseButton enabled="true" />
				</Button>

        <Button name="$(parent)_Color3" inherits="ZO_DefaultButton" text="3">
					<Anchor point="TOPLEFT" relativePoint="TOPLEFT" offsetX="40" relativeTo="$(parent)_Color2" />
					<Dimensions x="30" y="30" />
					<MouseButton enabled="true" />
				</Button>

        <Button name="$(parent)_Color4" inherits="ZO_DefaultButton" text="4">
					<Anchor point="TOPLEFT" relativePoint="TOPLEFT" offsetX="40" relativeTo="$(parent)_Color3" />
					<Dimensions x="30" y="30" />
					<MouseButton enabled="true" />
				</Button>

        <Button name="$(parent)_Color5" inherits="ZO_DefaultButton" text="5">
					<Anchor point="TOPLEFT" relativePoint="TOPLEFT" offsetX="40" relativeTo="$(parent)_Color4" />
					<Dimensions x="30" y="30" />
					<MouseButton enabled="true" />
				</Button>

        <Button name="$(parent)_ColorW" inherits="ZO_DefaultButton" text="W">
