							<Anchor point="TOPLEFT" relativeTo="$(parent)_MailLabel" relativePoint="TOPLEFT" offsetX="0" offsetY="40" />
					  	</Texture>

						<Label name="$(parent)_EMailKickLabel" font="ZoFontChat" mouseEnabled="true" wrapMode="ELLIPSIS" verticalAlignment="CENTER" text="Nachrichtenkick: ">
							<Anchor point="TOPLEFT" relativeTo="$(parent)_Line4" relativePoint="TOPLEFT" offsetX="0" offsetY="15" />

